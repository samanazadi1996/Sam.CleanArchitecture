{
  "$schema": "http://json.schemastore.org/template",
  "author": "Saman Azadi",
  "classifications": [
    "Clean Architecture"
  ],
  "name": "Clean Architecture Entity",
  "description": "Create a new Entity",
  "identity": "Clean.Architecture.Entity.CSharp",
  "groupIdentity": "Clean.Architecture.Entity",
  "shortName": "ca-entity",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "sourceName": "EntityName",
  "preferNameDirectory": false,
  "symbols": {
    "DefaultNamespace": {
      "type": "bind",
      "binding": "msbuild:RootNamespace",
      "replaces": "CleanArchitecture.Domain",
      "defaultValue": "CleanArchitecture.Domain"
    },
    "domainName": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "replaces": "DomainName",
      "fileRename": "DomainName"
    },
    "entityName": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "replaces": "EntityName",
      "fileRename": "EntityName"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "./**"
          ]
        }
      ]
    }
  ]
}